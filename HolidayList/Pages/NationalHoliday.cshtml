@page
@model HolidayList.Pages.NationalHolidaysModel
@{
    ViewData["Title"] = "NationalHolidays";
    var holidays = (holidaysNamespace.HolidayList)ViewData["Holidays"];
}

<div class="text-center">
    @* <h1 class="display-4">Welcome</h1>*@
    <hr />
    <table border="1" width="90%">
        <tr>
            <th>
                Name
            </th>
            <th>
                Description
            </th>
            <th>
                Date
            </th>
            <th>
                Date
            </th>
        </tr>

        @foreach (holidaysNamespace.Holiday Holidays in holidays.Response.Holidays)  
        {
            <tr>
                <td>@Holidays.Name</td>
                <td>@Holidays.Description</td>
                <td>@Holidays.Date.Iso</td>
            </tr>

        }
    </table>
</div>